
# ANT
CONFIG_ANT_CHECK=m
CONFIG_ANT_CHECK_DIV=m

# BATTERY
CONFIG_ONEWIRE_GPIO=m
CONFIG_BATT_VERIFY_BY_DS28E16=m

# CHARGER (NOPMI/SM5602/MAXIM)
CONFIG_NOPMI_CHARGER=m
# CONFIG_SMB1355_SLAVE_CHARGER is not set
# CONFIG_SMB1398_CHARGER is not set
CONFIG_FUELGAUGE_MAX77729=m
CONFIG_CHARGER_MAX77729=m
CONFIG_BQ2589X_CHARGER=m
CONFIG_CHARGER_SC8551=m
CONFIG_FG_SM5602=m
CONFIG_QCOM_PMIC_VOTER=m

# FINGERPRINT (FPC/GGODIX)
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_FPC1020=m
CONFIG_FINGERPRINT_GOODIX_FOD=m

# IR
CONFIG_SPI_SPIDEV=m

# IRQ
# CONFIG_QCOM_IRQ_STAT is not set

# LEDS/VIBRATOR
# CONFIG_LEDS_AW2016 is not set
# CONFIG_LEDS_QPNP_VIBRATOR_LDO is not set
CONFIG_LEDS_QPNP_VIBRATOR_PWM=m

# MINIDUMP
CONFIG_LAST_LOG_MINIDUMP=y
CONFIG_QCOM_MINIDUMP_LAST_KMSG=y

# PROJECT (SPES/SPESN)
CONFIG_TARGET_PROJECT_K7T=y

# REGULATOR
# CONFIG_REGULATOR_QCOM_PM8008 is not set

# SOC
# CONFIG_QCOM_FSA4480_I2C is not set

# SELINUX
CONFIG_SECURITY_SELINUX_DEVELOP=y

# THERMAL (MITHERMAL)
CONFIG_MI_THERMAL_INTERFACE=m

# USB (MAXIM/RICHTEK/XIAOMI_ADAPTER)
CONFIG_TCPC_CLASS=m
CONFIG_PD_DBG_INFO=y
CONFIG_TCPC_RT1711H=m
CONFIG_USB_POWER_DELIVERY=y
CONFIG_DUAL_ROLE_USB_INTF=m
CONFIG_CHARGER_PD_POLICY=m
CONFIG_MFD_MAX77729=m
CONFIG_CCIC_MAX77729=m
CONFIG_MUIC_MAX77729=m
